local Tab = Window:NewTab("SpectateüëÄ")
local Section = Tab:NewSection("Spectate")

local Players = game:GetService("Players")
local Camera = game.Workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local selectedPlayer = nil
local isSpectating = false

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Spectate
local function ToggleSpectate()
    if isSpectating then
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            Camera.CameraSubject = LocalPlayer.Character.Humanoid
        end
        isSpectating = false
        print("üëÄ ‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡∏°‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡πÅ‡∏•‡πâ‡∏ß")
    else
        if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("Humanoid") then
            Camera.CameraSubject = selectedPlayer.Character.Humanoid
            isSpectating = true
            print("üîç ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏î‡∏π‡∏°‡∏∏‡∏°‡∏°‡∏≠‡∏á‡∏Ç‡∏≠‡∏á: " .. selectedPlayer.Name)
        else
            warn("‚ùå ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ Spectate ‡πÑ‡∏î‡πâ‡∏Å‡πà‡∏≠‡∏ô")
        end
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function LoadPlayerList()
    local playerList = {}
    for _, v in pairs(Players:GetPlayers()) do
        if v ~= LocalPlayer then
            table.insert(playerList, v.Name)
        end
    end
    return playerList
end

-- Dropdown ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local playerList = LoadPlayerList()
local dropdown = Section:NewDropdown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô", "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏∞ Spectate", playerList, function(selected)
    selectedPlayer = Players:FindFirstChild(selected)
    print("‚úî ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô: " .. selected)
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠ Spectate/‡∏´‡∏¢‡∏∏‡∏î
Section:NewButton("‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏î‡∏π/‡∏´‡∏¢‡∏∏‡∏î", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏´‡∏£‡∏∑‡∏≠‡∏´‡∏¢‡∏∏‡∏î Spectate", function()
    ToggleSpectate()
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
Section:NewButton("üîÑ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠", "‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏´‡∏°‡πà", function()
    playerList = LoadPlayerList()
    dropdown:Refresh(playerList)
    print("üîÑ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß")
end)

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤/‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    playerList = LoadPlayerList()
    dropdown:Refresh(playerList)
end)

Players.PlayerRemoving:Connect(function()
    playerList = LoadPlayerList()
    dropdown:Refresh(playerList)
end)

local Tab = Window:NewTab("TeleportüåÄ")
local Section = Tab:NewSection("Teleport")

local selectedPlayer = nil
local teleporting = false
local smoothing = false
local noclip = false
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = char:WaitForChild("HumanoidRootPart")

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î NoClip
local function setNoClip(state)
    noclip = state
    if noclip then
        game:GetService("RunService").Stepped:Connect(function()
            if noclip and char then
                for _, part in pairs(char:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    end
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô (‡πÑ‡∏°‡πà‡∏£‡∏ß‡∏°‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á)
local dropdown
local function updatePlayerList()
    local playerNames = {}
    for _, plr in pairs(game.Players:GetPlayers()) do
        if plr ~= player then -- ‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á
            table.insert(playerNames, plr.Name)
        end
    end
    
    if dropdown then
        dropdown:Refresh(playerNames)
    else
        dropdown = Section:NewDropdown("Select Player", "Choose a player to teleport", playerNames, function(currentOption)
            selectedPlayer = currentOption
            print("Selected Player: " .. selectedPlayer)
        end)
    end
end

updatePlayerList()

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô‡∏ï‡∏≤‡∏°‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏ö‡∏ö‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡∏ß‡∏≤‡∏õ‡∏Å‡πà‡∏≠‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡∏ö‡∏¥‡∏ô)
local function startFollowing(target)
    if not target or not target.Character then return end

    humanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame

    local bodyPos = Instance.new("BodyPosition", humanoidRootPart)
    bodyPos.MaxForce = Vector3.new(1e5, 1e5, 1e5)
    bodyPos.P = 10e4
    bodyPos.D = 100

    teleporting = true
    while teleporting and task.wait() do
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            bodyPos.Position = target.Character.HumanoidRootPart.Position
        else
            teleporting = false
        end
    end

    bodyPos:Destroy()
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏ï‡∏≤‡∏°‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏ö‡∏ö Smooth
local function smoothFollow(target)
    if not target or not target.Character then return end

    -- ‡πÄ‡∏õ‡∏¥‡∏î NoClip
    setNoClip(true)

    local bodyVel = Instance.new("BodyVelocity", humanoidRootPart)
    bodyVel.MaxForce = Vector3.new(1e5, 1e5, 1e5)

    smoothing = true
    while smoothing and task.wait() do
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            local direction = (target.Character.HumanoidRootPart.Position - humanoidRootPart.Position).unit
            bodyVel.Velocity = direction * 50
        else
            smoothing = false
        end
    end

    bodyVel:Destroy()
    setNoClip(false) -- ‡∏õ‡∏¥‡∏î NoClip ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡∏ö‡∏¥‡∏ô
end

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏£‡∏¥‡πà‡∏°/‡∏´‡∏¢‡∏∏‡∏î‡∏ö‡∏¥‡∏ô‡∏ï‡∏≤‡∏°‡πÅ‡∏ö‡∏ö‡∏ß‡∏≤‡∏õ‡∏Å‡πà‡∏≠‡∏ô
local teleportButton
teleportButton = Section:NewButton("Start Teleport OP", "Teleport and follow selected player", function()
    if selectedPlayer then
        teleporting = not teleporting
        local targetPlayer = game.Players:FindFirstChild(selectedPlayer)
        if teleporting and targetPlayer then
            teleportButton:UpdateButton("Stop Teleport OP")
            startFollowing(targetPlayer)
        else
            teleportButton:UpdateButton("Start Teleport OP")
        end
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏£‡∏¥‡πà‡∏°/‡∏´‡∏¢‡∏∏‡∏î‡∏ö‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏ (Smooth Teleport)
local smoothButton
smoothButton = Section:NewButton("Smooth Teleport", "Fly smoothly through walls to target", function()
    if selectedPlayer then
        smoothing = not smoothing
        local targetPlayer = game.Players:FindFirstChild(selectedPlayer)
        if smoothing and targetPlayer then
            smoothButton:UpdateButton("Stop Smooth Teleport")
            smoothFollow(targetPlayer)
        else
            smoothButton:UpdateButton("Smooth Teleport")
        end
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
Section:NewButton("Refresh Players", "Update player list", function()
    updatePlayerList()
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î/‡πÄ‡∏õ‡∏¥‡∏î UI ‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏£‡∏π‡∏õ‡∏†‡∏≤‡∏û ‡πÅ‡∏•‡∏∞‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏•‡∏≤‡∏Å‡πÑ‡∏î‡πâ
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui -- ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏´‡πâ UI ‡∏Ñ‡∏á‡∏≠‡∏¢‡∏π‡πà‡∏ï‡∏•‡∏≠‡∏î‡πÄ‡∏ß‡∏•‡∏≤

local ToggleButton = Instance.new("ImageButton")
ToggleButton.Parent = ScreenGui
ToggleButton.Size = UDim2.new(0, 50, 0, 50)
ToggleButton.Position = UDim2.new(0.1, 0, 0.1, 0)  -- ‡∏õ‡∏£‡∏±‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡πâ‡∏õ‡∏∏‡πà‡∏°‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏ã‡πâ‡∏≤‡∏¢
ToggleButton.BackgroundTransparency = 1
ToggleButton.Image = "rbxassetid://78464547405474"
ToggleButton.Draggable = true
ToggleButton.Active = true
ToggleButton.MouseButton1Click:Connect(function()
    Library:ToggleUI()
end)
